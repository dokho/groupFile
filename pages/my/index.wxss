page {
  background-color: #fff;
  font-size: 32rpx;
}

button::after{
	border: none;
}

.loginButton {
	padding-left: 0rpx;
	padding-right: 0rpx;
	text-align: left;
	background-color: #fff;
	color: #fff;
	font-size: 30rpx;
	line-height: 50rpx;
	background-color: #FD5746;
	border-radius: 0rpx;
}

.row {
	width: 100%;
	height: 110rpx;
	background-color: #fff;
	margin-bottom: 2rpx;
	padding: 40rpx 35rpx;
	color:#FD5746;
	font-size: 30rpx;
	border-bottom: 2rpx solid #f5f5f5;
}

.row .icon {
	width: 36rpx;
	height: 36rpx;
	vertical-align: middle;
}